# Aetheris 环境配置示例
# 复制此文件为 .env 并根据实际情况修改配置

# 服务配置
HOST=127.0.0.1
PORT=8000
DEBUG=True

# CORS配置（多个地址用逗号分隔）
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# AI配置（可选）
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo

# 缓存配置
CACHE_TYPE=memory
CACHE_TTL=3600

# 日志配置
LOG_LEVEL=INFO
